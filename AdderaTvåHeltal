/*Addera tv책 tal oc skriv ut summan*/
#include "stdafx.h"
#include "stdio.h"
#include "stdlib.h"

int main()
{
	int tal1 = 0;
	int tal2 = 0;
	int nytt_tal;
	int summa;
	
	printf("Addera tv책 tal och f책 ut summan");
	nytt_tal = scanf_s("%d %d", &tal1, &tal2);
	getchar();
	summa = tal1 + tal2;
	printf_s("Summan av de talen %d och %d 채r %d", tal1, tal2, summa);
	getchar(); 
    return 0;
}
